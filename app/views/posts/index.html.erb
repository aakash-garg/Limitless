<%= link_to "Sign Out", destroy_user_session_path, :method => :delete %>
<%= link_to "New Post", new_post_path %>
<% @posts.each do |post| %>
    <div>
      <h3>
        <span>
          <%= link_to post.title, post %>
        </span>
        <span>
          <small>
            --
          <%= post.user.email %>
          </small>
        </span>
      </h3>
      <p>
        <%= post.created_at.strftime("%Y-%m-%d")  %>
      </p>
    </div>
<% end %>