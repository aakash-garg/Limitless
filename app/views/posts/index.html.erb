<p>
<%= link_to "Sign Out", destroy_user_session_path, :method => :delete %>
</p>
<p>
<%= link_to "New Post", new_post_path %>
</p>
<p>
  <%= link_to "Edit User", edit_user_registration_path %>
</p>
<% @posts.each do |post| %>
    <div>
      <h3>
        <span>
          <%= link_to post.title, post %>
        </span>
        <span>
          <small>
            --
          <%= post.user.email %>
          </small>
        </span>
      </h3>
      <p>
        <%= post.created_at.strftime("%Y-%m-%d")  %>
      </p>
    </div>
<% end %>